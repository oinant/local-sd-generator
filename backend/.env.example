# Configuration des GUIDs d'authentification
# Copiez ce fichier vers .env et remplacez par vos propres GUIDs

# GUIDs valides pour l'authentification (format JSON array)
VALID_GUIDS=["admin-guid-here", "user1-guid-here", "user2-guid-here", "readonly-user-guid-here"]

# GUIDs ayant accès en lecture seule (format JSON array)
READ_ONLY_GUIDS=["readonly-user-guid-here"]

# Configuration Stable Diffusion WebUI
SD_WEBUI_URL="http://127.0.0.1:7860"

# Configuration des dossiers pour le treeview (format JSON array sur une seule ligne)
# Chaque dossier a un nom, un chemin (absolu ou relatif) et un type
# IMPORTANT: Échapper les backslashes Windows avec \\
IMAGE_FOLDERS=[{"name": "Sessions", "path": "./apioutput/sessions", "type": "sessions"}, {"name": "Archives", "path": "/path/to/your/archives", "type": "folder"}, {"name": "Favoris", "path": "/home/user/images/favorites", "type": "folder"}]