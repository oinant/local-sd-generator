{
  "version": "1.0",
  "name": "Interactive Example",
  "description": "Configuration with interactive prompts for testing",

  "prompt": {
    "template": "{Expression}, {Angle}, portrait, detailed face",
    "negative": "low quality, blurry"
  },

  "variations": {
    "Expression": "/mnt/d/StableDiffusion/local-sd-generator/variations/test_expressions.txt",
    "Angle": "/mnt/d/StableDiffusion/local-sd-generator/variations/test_angles.txt"
  },

  "generation": {
    "mode": "ask",
    "seed_mode": "ask",
    "seed": -1,
    "max_images": -1
  },

  "parameters": {
    "width": 512,
    "height": 768,
    "steps": 20,
    "cfg_scale": 7.0,
    "sampler": "ask",
    "batch_size": 1,
    "batch_count": 1
  },

  "output": {
    "session_name": "interactive_example",
    "filename_keys": ["Expression", "Angle"]
  }
}
