{
  "version": "1.0",
  "name": "Phase 2 Integration Test",
  "description": "Test configuration to validate SF-7 and SF-1 implementation",

  "model": {
    "checkpoint": "animePastelDream_v1.safetensors"
  },

  "prompt": {
    "template": "masterpiece, best quality, {Expression}, {Angle}, 1girl, anime style, detailed face",
    "negative": "low quality, blurry, bad anatomy, text, watermark, jpeg artifacts"
  },

  "variations": {
    "Expression": "/mnt/d/StableDiffusion/local-sd-generator/variations/test_expressions.txt",
    "Angle": "/mnt/d/StableDiffusion/local-sd-generator/variations/test_angles.txt"
  },

  "generation": {
    "mode": "combinatorial",
    "seed_mode": "progressive",
    "seed": 42,
    "max_images": 20
  },

  "parameters": {
    "width": 512,
    "height": 768,
    "steps": 30,
    "cfg_scale": 7.5,
    "sampler": "DPM++ 2M Karras",
    "batch_size": 1,
    "batch_count": 1
  },

  "output": {
    "session_name": "phase2_integration_test",
    "filename_keys": ["Expression", "Angle"]
  }
}
